[global_config]
  dbus = True
  borderless = True
  handle_size = 0
  focus = mouse
[keybindings]
[profiles]
  [[default]]
    use_system_font = False
    font = Monospace 10
    background_darkness = 0.52
    scrollback_lines = 9999
    background_type = transparent
    scrollbar_position = hidden
    cursor_color = "#ff00ff"
    foreground_color = "#ffffff"
    scroll_on_output = False
    show_titlebar = False
    copy_on_selection = True
    cursor_blink = False
  [[small font]]
    use_system_font = False
    font = Monospace 8
    scrollback_lines = 9999
    background_type = transparent
    scrollbar_position = hidden
    cursor_color = "#ff00ff"
    foreground_color = "#ffffff"
    scroll_on_output = False
    show_titlebar = False
    copy_on_selection = True
    cursor_blink = False
  [[matrix]]
    use_system_font = False
    background_darkness = 0.0
    background_type = transparent
    scrollbar_position = hidden
    foreground_color = "#ffffff"
    show_titlebar = False
    antialias = False
    font = Source Code Pro Heavy 6
  [[dark transparency]]
    use_system_font = False
    font = Monospace 10
    background_darkness = 0.8
    scrollback_lines = 9999
    background_type = transparent
    scrollbar_position = hidden
    cursor_color = "#ff00ff"
    foreground_color = "#ffffff"
    scroll_on_output = False
    show_titlebar = False
    copy_on_selection = True
    cursor_blink = False
[layouts]
  [[default]]
    [[[child1]]]
      profile = default
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
